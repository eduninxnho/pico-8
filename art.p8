version 42
__lua__


function _init()
	cls(-15)
	sprp=2
	sprp1=2
	speed=2
	x=40
	y=40
	by=y-2 																		
end

function _update()

		if x>128 then
		x=x-142
		end
		
		if x<-14 then
		x=x+142
		end
		
		if btn(1) then
x=x+speed

		end
		if btn(0) then
x=x-speed

		end
		if btn(3) then
y=y+1

		end
		if btn(2) then
y=y-speed

		end
		
		function _draw()
	cls(-15)
	if btn(4) then
print("nao",x-14,y-10,8)

		end
		if btnp(5) then
spr(8,x+4,by)

		end
	spr(1,x,y,sprp,sprp1)

end


end
